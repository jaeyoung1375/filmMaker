<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="member">
	<insert id="join">
		insert into member values(member_seq.nextval, #{memberName}, #{memberEmail}, #{memberPassword}, #{memberTel}, #{memberBirth}, "일반")
	</insert>
</mapper>